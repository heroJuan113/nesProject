{{> navegacion/navegacion}}
<div class="container p-5">
    <div class="row">
        <div class="col-md-3">
            <div class="card">
                {{#each cliente}}
                <form action="http://127.0.0.1:4000/clientes/editar/{{idCliente}}/{{idUsuario}}/{{idSession}}" method="POST">
                    <div class="card-body">{{id}}
                             {{> clientes/formularioEdit}}
                            {{> botones/aceptar }}
                    </div>
                </form>
                {{/each}}
            </div>
        </div>
    </div>
</div>
